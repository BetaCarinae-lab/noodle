mut number timestoloop is 3

loop <timestoloop> {
    writeLn("This will repeat " + <timestoloop> + " times")
}

while(<timestoloop> > 0) {
    writeLn("While loop will also repeat 3 times")
    timestoloop is <timestoloop> - 1
}

array arr is [1, 3, 9, 7, 2]

writeLn("foreach:")

foreach(<arr>, index) {
    writeLn("Index: " + <index>)
    writeLn("Value: " + <arr at <index>>)
}

number x is 0

while (<x> < 10) {
    x is <x> + 1

    if (<x> == 3) {
        continue
    }

    if (<x> == 7) {
        break
    }

    writeLn(<x>)
}
