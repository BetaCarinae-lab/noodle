template MyStruct {
    prop property is number,
    prop property2 is number,
    method greet is () {
        writeLn("Hello")
    }
    method echoProperty is () {
        writeLn(<self.property>)
    }
    method bumpProperty is () {
        self.property is 200
    }
    method echoSelf is () {
        <inspect#(<self>)>
    }
}

object a is MyStruct {
    property: 10,
    property2: 20,
}

<a.greet()>

writeLn(<a.property> + <a.property2>)

<a.echoProperty()>
<a.bumpProperty()>
<a.echoSelf()>

writeLn("PROP: " + <a.property>)

writeLn("PROP_2: " + <<a>.property2>)

writeLn("OBJ END")